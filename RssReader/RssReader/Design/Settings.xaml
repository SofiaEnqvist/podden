<Window x:Class="RssReader.Design.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Settings" Height="300" Width="300">
    <Grid>
        <TextBox Name="category" HorizontalAlignment="Left" Height="23" Margin="10,30,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="160,33,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <ComboBox HorizontalAlignment="Left" Margin="10,101,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Delete" HorizontalAlignment="Left" Margin="160,101,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Change" HorizontalAlignment="Left" Margin="160,131,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.192,-0.02" Height="24" Click="Button_Click_2"/>
        <TextBox Name="Newcategory" HorizontalAlignment="Left" Height="23" Margin="10,132,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
