<Window x:Class="RssReader.Design.AddSubscription"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AddSubscription" Height="300" Width="300" Loaded ="AddSubscription_OnLoad">
    <Grid>
        <ComboBox  Name="cbCategory" HorizontalAlignment="Left" Margin="141,51,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cbCategory_SelectionChanged"/>
        <Label Content="Lägg till i kategori: " HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top"/>
        <Label Content="Välj ett uppdateringsintervall" HorizontalAlignment="Left" Margin="10,98,0,0" VerticalAlignment="Top"/>
        <ComboBox Name="cbInterval" HorizontalAlignment="Left" Margin="200,102,0,0" VerticalAlignment="Top" Width="62"/>
        <Button Name="btnExit" Content="Avbryt" HorizontalAlignment="Left" Margin="46,188,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Name="btnOkej" Content="Lägg till" HorizontalAlignment="Left" Margin="160,188,0,0" VerticalAlignment="Top" Width="75" Click="btnOkej_Click"/>

    </Grid>
</Window>
